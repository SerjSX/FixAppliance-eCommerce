{
  "name": "fixappliance-frontend",
  "version": "1.0.0",
  "description": "FixAppliance Frontend - Vue.js with TailwindCSS",
  "private": true,
  "scripts": {
    "build:css": "npx tailwindcss -i ./styles/main.css -o ./dist/output.css --watch",
    "build:css:prod": "npx tailwindcss -i ./styles/main.css -o ./dist/output.css --minify"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "pinia": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "preline": "^2.4.0",
    "tailwindcss": "^3.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0"
  }
}
